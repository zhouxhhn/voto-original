<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
    <class name="bjl.domain.model.profitrecord.ProfitRecord" table="t_profit_record">
        <id name="id" type="java.lang.String" column="id" unsaved-value="-1" length="32">
            <generator class="uuid"/>
        </id>

        <version name="version" type="java.lang.Integer" column="version"/>

        <property name="createDate" column="create_date" type="java.util.Date"/>

        <property name="init" column="init" type="java.lang.Integer"/>

        <property name="profit" column="profit" type="java.lang.Integer"/>

    </class>

</hibernate-mapping>