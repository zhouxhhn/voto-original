<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="bjl.domain.model.upDownPoint.UpDownPoint" table="t_up_down_point" lazy="true">
        <id name="id" type="java.lang.String" column="id" unsaved-value="null" length="32">
            <generator class="uuid"/>
        </id>
        <version column="version" name="version" type="java.lang.Integer"/>
        <property name="userName" column="user_name" type="java.lang.String"/>
        <property name="createDate" column="create_date" type="java.util.Date"/>
        <property name="upDownPoint" column="up_down_point" type="java.math.BigDecimal"/>
        <property name="operationUser" column="operation_user" type="java.lang.String"/>
        <property name="upDownPointType" column="up_down_point_type" type="java.lang.Integer"/>
        <property name="name" column="name" type="java.lang.String"/>
    </class>
</hibernate-mapping>